#!/bin/bash

export SCRIPT_DIR=$(dirname "$0")

##
## Build Process
##

main ()
{
    cd src/Connect 
    xcodebuild test -project Connect.xcodeproj/ -scheme Connect -sdk iphonesimulator -destination "platform=iOS Simulator,OS=9.3,name=iPhone 4s" ONLY_ACTIVE_ARCH=NO 1>/dev/null

    local status=$?

    exit $status
}

main
